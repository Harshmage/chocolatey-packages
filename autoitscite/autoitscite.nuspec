<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>autoitscite</id>
		<title>AutoIt SciTE Editor</title>
		<version>3.4.4</version>
		<authors>Neil Hodgson</authors>
		<owners>Harshmage</owners>
		<summary>Custom version of SciTE for AutoIt scripting</summary>
		<description>While AutoIt comes with a version of SciTE for editing and compile features, the custom standalone version is much more feature rich, and allows for more inline text completion.</description>
		<projectUrl>https://github.com/Harshmage/chocolatey-packages</projectUrl>
		<tags>autoit install automation tool editor</tags>
		<copyright></copyright>
		<licenseUrl>http://www.scintilla.org/License.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<iconUrl>https://cdn.rawgit.com/Harshmage/chocolatey-packages/master/icons/autoitscite.png</iconUrl>
		<dependencies>
			<dependency id='autoit' />
		</dependencies>
		<releaseNotes></releaseNotes>
	</metadata>
	<files>
		<file src="tools\**" target="tools" />
	</files>
</package>